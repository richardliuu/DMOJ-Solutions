n = int(input())

# Outer loop 
for i in range(n):
    line = input().strip()
    count = 1

    # To track the final result after counting 
    result = []

    # Inner loop for the characters 
    for j in range(1, len(line)):
        if line[j] == line[j-1]:
            count += 1
        else:
            result.append(f"{count} {line[j-1]}")
            count = 1

    result.append(f"{count} {line[-1]}")
    print(" ".join(result))
