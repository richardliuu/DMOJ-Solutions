class Solution:
    def countNegatives(self, grid: List[List[int]]) -> int:
        count = 0

        # Outer loop to loop through lists in lists 
        for j in range(len(grid)):

            # Loop for items in lists 
            for i in range(len(grid[j])):
                if grid[j][i] < 0:
                    count += 1 

        return count 
