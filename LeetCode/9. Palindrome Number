class Solution:
    def isPalindrome(self, x: int) -> bool:
        rev_num = 0 
        num = x 

        while num > 0:
            digit = num % 10
            rev_num = rev_num * 10 + digit
            num = num // 10

        if rev_num == x:
            return True
        else: 
            return False 

        
        

