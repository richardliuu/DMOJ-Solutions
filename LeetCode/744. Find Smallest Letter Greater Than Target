class Solution:
    def nextGreatestLetter(self, letters: List[str], target: str) -> str:        
        length = len(letters)
        left = 0         
        right = length - 1 

        if target >= letters[-1] or target < letters[0]:
            return letters[0]

        while left <= right:
            mid = (left+right) // 2
            if target >= letters[mid]:
                left = mid + 1
            else:
                right = mid - 1

        return letters[left]
